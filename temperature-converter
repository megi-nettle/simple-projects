function TheConverter () {
  var celsius = prompt('What is the temperature outside, in Â°C?');
  var fahrenheit = celsius * 9 / 5 +32;
  alert('That is ' + fahrenheit + ' degrees Fahrenheit.');
  return; 
};

TheConverter();

